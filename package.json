{
  "name": "sfconfig",
  "version": "0.0.9",
  "description": "Lightweight utility for managing generative AI CLI configurations",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "sfconfig": "bin/sfconfig.js"
  },
  "files": [
    "bin/",
    "apps/web/dist/",
    "apps/web/dist-server/", 
    "apps/web/packages/",
    "packages/core/dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowfort-ai/config.git"
  },
  "bugs": {
    "url": "https://github.com/snowfort-ai/config/issues"
  },
  "homepage": "https://github.com/snowfort-ai/config#readme",
  "scripts": {
    "dev": "pnpm --recursive run dev",
    "build": "pnpm --recursive run build",
    "prepublishOnly": "pnpm build && node scripts/simple-bundle.js && node scripts/fix-imports.js",
    "start:web": "./start-web.sh",
    "test": "pnpm --recursive run test",
    "lint": "pnpm --recursive run lint",
    "typecheck": "pnpm --recursive run typecheck",
    "clean": "pnpm --recursive run clean"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "playwright": "^1.53.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "configuration",
    "cli",
    "management"
  ],
  "author": "Snowfort",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "ajv": "^8.12.0",
    "chokidar": "^3.5.3",
    "update-notifier": "^7.0.0",
    "yaml": "^2.3.4",
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "@heroicons/react": "^2.2.0",
    "express": "^4.18.2",
    "react-dom": "^18.2.0",
    "open": "^10.0.0"
  }
}
