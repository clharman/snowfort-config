{
  "name": "@snowfort/config-web",
  "version": "0.0.1",
  "description": "Web UI for Snowfort Config",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.server.json",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist-server/index.js",
    "start:wait": "node dist-server/index.js & sleep 1 && until curl -s http://localhost:4040/api/health | grep -q '\"status\":\"ready\"'; do sleep 0.5; done && echo 'ðŸš€ Server is ready!' && wait",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-server"
  },
  "dependencies": {
    "@snowfort/config-core": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.0"
  }
}